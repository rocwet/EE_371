    .scope S_008d4dd8;
T_0 ;
    %wait E_00913470;
    %load/vec4 v008d4f00_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v008d4ea8_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v008d4ea8_0;
    %parti/s 1, 0, 2;
    %inv;
    %ix/load 4, 3, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v008d4ea8_0, 4, 5;
    %load/vec4 v008d4ea8_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v008d4ea8_0, 4, 5;
    %load/vec4 v008d4ea8_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v008d4ea8_0, 4, 5;
    %load/vec4 v008d4ea8_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v008d4ea8_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_008deb50;
T_1 ;
