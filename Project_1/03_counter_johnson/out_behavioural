    .scope S_008deb50;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00911ad0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00911bd8_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00911bd8_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00911b28_0, 0, 32;
T_2.0 ;
